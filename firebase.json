{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/www.coderfarm.in",
        "destination": "https://coderfarm.in",
        "type": 301
      },
      {
        "source": "/http://www.coderfarm.in",
        "destination": "https://coderfarm.in",
        "type": 301
      },
      {
        "source": "/https://www.coderfarm.in",
        "destination": "https://coderfarm.in",
        "type": 301
      }
    
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
